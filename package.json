    "babel-register": "^6.24.0",
    "babel-runtime": "^6.11.6",
    "chai": "^4.0.2",
    "chromedriver": "^2.40.0",
    "codecov": "^3.0.1",
    "commitizen": "^2.9.6",
    "copy-webpack-plugin": "^4.5.1",
    "core-js": "^2.5.5",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "cz-conventional-changelog": "^2.1.0",
    "dotenv": "^6.0.0",
    "errorshots": "kontrollanten/errorshots#display-uploaded-url",
    "eslint": "^5.0.0",
    "eslint-plugin-compat": "^2.4.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-react": "^7.9.1",
    "extract-text-webpack-plugin": "^4.0.0-alpha.0",
    "fetch-mock": "^6.4.4",
    "file-loader": "^1.1.10",
