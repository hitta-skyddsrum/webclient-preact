    "webpack-dev-server": "^3.1.3"
  },
  "dependencies": {
    "algolia-places-react": "^1.3.0",
    "classnames": "^2.2.6",
    "dynamic-polyfill": "^1.1.0",
    "history": "^4.7.2",
    "leaflet": "^1.3.1",
    "leaflet.markercluster": "^1.3.0",
    "material-ui": "^1.0.0-beta.43",
    "material-ui-bottom-sheet": "^0.5.1",
    "material-ui-icons": "^1.0.0-beta.36",
    "polyline": "^0.2.0",
    "preact": "^8.2.9",
