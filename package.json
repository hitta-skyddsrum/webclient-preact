    "offline-plugin": "^5.0.5",
    "postcss-loader": "^3.0.0",
    "preact-jsx-chai": "^2.2.1",
    "preact-render-spy": "^1.3.0",
    "preact-test-utils": "^0.1.3",
    "raw-loader": "^0.5.1",
    "redux-mock-store": "^1.5.3",
    "regenerator-runtime": "^0.11.1",
    "replace-bundle-webpack-plugin": "^1.0.0",
    "sass": "^1.7.0",
