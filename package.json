{
  "name": "hitta-skyddsrum-client",
  "version": "2.0.0",
  "description": "",
  "scripts": {
    "precommit": "yarn lint",
    "commit": "git-cz",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --progress",
    "e2e": "curl -o /dev/null $SITE_URL && yarn nightwatch -- -e chrome",
    "install-selenium": "selenium-standalone install",
    "lh": "lighthouse-ci",
    "nightwatch": "nightwatch",
    "start": "serve -s -l 5000 build",
    "prestart": "yarn build",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "prebuild": "mkdirp build && ncp src/assets build/assets",
    "test": "yarn -s lint && jest",
    "test:ci": "jest --coverage",
    "test:watch": "yarn -s lint && jest --watch",
    "lint": "eslint src test",
    "postinstall": "npm rebuild node-sass --update-binary"
  },
  "keywords": [
    "preact",
    "boilerplate",
    "webpack"
  ],
  "license": "MIT",
  "author": "Hitta Skyddsrum <admin@hittaskyddsrum.se>",
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ],
    "testURL": "http://localhost:8080",
    "transformIgnorePatterns": [
      "/node_modules/(?!(@material|preact-material-components/node_modules/@material)/).*/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy",
      "^react$": "preact-compat",
      "^react-dom$": "preact-compat"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/reducer.js",
      "!src/pwa.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.4.0",
