{
  "name": "hitta-skyddsrum-client",
  "version": "2.0.0",
  "description": "",
  "scripts": {
    "precommit": "npm run lint",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --progress",
    "e2e": "nightwatch -e chrome",
    "install-selenium": "selenium-standalone install",
    "start": "serve build -s -c 1",
    "prestart": "npm run build",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "prebuild": "mkdirp build && ncp src/assets build/assets",
    "test": "npm run -s lint && jest --coverage",
    "test:watch": "npm run -s lint && jest --watch",
    "lint": "eslint src test",
    "postinstall": "npm rebuild node-sass"
  },
  "keywords": [
    "preact",
    "boilerplate",
    "webpack"
  ],
  "license": "MIT",
  "author": "Hitta Skyddsrum <admin@hittaskyddsrum.se>",
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ],
    "testURL": "http://localhost:8080",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy",
      "^react$": "preact-compat",
      "^react-dom$": "preact-compat"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/reducer.js",
      "!src/pwa.js"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "babel": "^6.5.2",
    "babel-core": "^6.24.0",
    "babel-eslint": "^8.2.2",
