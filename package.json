    "chromedriver": "^2.36.0",
    "codecov": "^3.0.0",
    "copy-webpack-plugin": "^4.4.2",
    "core-js": "^2.4.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.10",
    "dotenv": "^5.0.1",
    "errorshots": "kontrollanten/errorshots#display-uploaded-url",
    "eslint": "^4.18.1",
    "eslint-plugin-jest": "^21.13.0",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-alpha.0",
