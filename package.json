{
  "name": "hitta-skyddsrum-client",
  "version": "2.0.0",
  "description": "",
  "scripts": {
    "precommit": "npm run lint",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --progress",
    "e2e": "nightwatch",
    "install-selenium": "selenium-standalone install",
    "start": "serve build -s -c 1",
    "prestart": "npm run build",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "prebuild": "mkdirp build && ncp src/assets build/assets",
    "test": "npm run -s lint && jest --coverage",
    "test:watch": "npm run -s lint && jest --watch",
    "lint": "eslint src test"
  },
  "keywords": [
    "preact",
    "boilerplate",
    "webpack"
  ],
  "license": "MIT",
  "author": "Hitta Skyddsrum <admin@hittaskyddsrum.se>",
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ],
    "testURL": "http://localhost:8080",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy",
      "^react$": "preact-compat",
      "^react-dom$": "preact-compat"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/reducer.js"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^7.0.1",
    "babel": "^6.5.2",
    "babel-core": "^6.24.0",
    "babel-eslint": "8.1.2",
    "babel-jest": "22.0.4",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.24.0",
    "babel-runtime": "^6.11.6",
    "chai": "^4.0.2",
    "chromedriver": "^2.34.0",
    "codecov": "^3.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.1.0",
    "eslint-plugin-jest": "21.5.0",
    "eslint-plugin-react": "^7.0.0",
    "extract-text-webpack-plugin": "3.0.2",
    "fetch-mock": "^5.13.1",
    "file-loader": "1.1.6",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "22.0.4",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "nightwatch": "^0.9.19",
    "node-sass": "^4.5.3",
    "offline-plugin": "^4.7.0",
    "postcss-loader": "^2.0.3",
    "preact-jsx-chai": "^2.2.1",
    "preact-render-spy": "^1.2.1",
    "preact-test-utils": "^0.1.3",
    "raw-loader": "^0.5.1",
    "redux-mock-store": "^1.4.0",
    "regenerator-runtime": "0.11.1",
    "replace-bundle-webpack-plugin": "^1.0.0",
    "sass": "^1.0.0-beta.2",
    "sass-loader": "^6.0.6",
    "script-ext-html-webpack-plugin": "^1.3.4",
    "selenium-standalone": "^6.12.0",
    "sinon": "4.1.3",
    "sinon-chai": "^2.9.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.19.1",
    "url-loader": "0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "core-decorators": "^0.20.0",
    "debounce": "^1.1.0",
    "leaflet": "^1.2.0",
    "material-ui": "^0.20.0",
    "material-ui-bottom-sheet": "^0.5.1",
    "polyline": "^0.2.0",
    "preact": "^8.2.6",
    "preact-compat": "^3.15.0",
    "preact-redux": "^2.0.3",
    "preact-render-to-string": "^3.6.0",
    "preact-router": "^2.6.0",
    "promise-polyfill": "7.0.0",
    "proptypes": "^1.0.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-leaflet": "^1.7.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "rxjs": "^5.5.2",
    "serve": "^6.0.0",
    "whatwg-fetch": "^2.0.3"
  }
}
