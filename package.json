    "preact-redux": "^2.0.3",
    "preact-render-to-string": "^3.6.0",
    "preact-router": "^2.6.1",
    "preact-router-redux": "^4.1.0",
    "raven-for-redux": "^1.3.1",
    "raven-js": "^3.26.3",
    "react-custom-scrollbars": "^4.2.1",
    "react-leaflet": "^1.9.1",
    "react-leaflet-markercluster": "^1.1.8",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
